{
  "name": "frictionless-ui",
  "version": "0.1.0",
  "license": "MIT",
  "author": "roll <eskarev@gmail.com>",
  "description": "UI components for frictionless",
  "homepage": "https://github.com/frictionlessdata/frictionless-ui",
  "bugs": "https://github.com/frictionlessdata/frictionless-ui/issues",
  "repository": "github:frictionlessdata/frictionless-ui",
  "main": "dist/frictionless-ui.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "analyze": "NODE_ENV=production webpack --profile --json | webpack-bundle-size-analyzer",
    "build": "npm run compile && npm run bundle",
    "bundle": "webpack --output-pathinfo --progress --hide-modules && NODE_ENV=production webpack --progress --hide-modules",
    "compile": "rm -rf lib && tsc && cp -r src/images lib && cp -r src/styles lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "webpack serve --inline --hot --open",
    "e2e": "cypress run",
    "format": "prettier --write 'src/**/*.ts*' && eslint --fix 'src/**/*.ts*'",
    "lint": "prettier --check 'src/**/*.ts*' && eslint 'src/**/*.ts*'",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "readme": "doctoc --maxlevel 3 README.md",
    "test": "jest --coverage",
    "update": "ncu -u"
  },
  "dependencies": {
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.168",
    "@types/marked": "^2.0.1",
    "classnames": "^2.3.1",
    "hex-to-rgba": "^2.0.1",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "marked": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "compression-webpack-plugin": "^7.1.2",
    "coveralls": "^3.1.0",
    "css-loader": "^5.2.0",
    "cypress": "^7.0.1",
    "doctoc": "^2.0.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "npm-check-updates": "^11.4.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^17.0.2",
    "referencer": "^0.2.5",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
